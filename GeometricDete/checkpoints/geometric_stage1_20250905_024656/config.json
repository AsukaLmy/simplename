{
  "data_path": "../dataset",
  "batch_size": 64,
  "num_workers": 2,
  "model_type": "adaptive",
  "hidden_dims": [
    32,
    16
  ],
  "hidden_size": 16,
  "history_length": 5,
  "num_ensemble_models": 3,
  "dropout": 0.1,
  "epochs": 50,
  "learning_rate": 0.001,
  "weight_decay": 0.0001,
  "optimizer": "adam",
  "scheduler": "step",
  "step_size": 20,
  "weight_regularization": 0.01,
  "sparsity_regularization": 0.01,
  "max_grad_norm": 1.0,
  "early_stopping_patience": 15,
  "log_interval": 10,
  "use_temporal": true,
  "use_scene_context": true
}